CREATE TABLE [dbo].[Acudiente](
	[Id_Acudiente] [int] IDENTITY(1,1) NOT NULL,
	[Dni] [varchar](20) NOT NULL,
	[Tipo_documento] [varchar](20) NOT NULL,
	[Nombre_apellidos] [varchar](100) NOT NULL,
	[Fecha_nacimiento] [date] NOT NULL,
	[Direccion_casa] [varchar](20) NOT NULL,
	[TelefonoCasa] [varchar](20) NULL,
	[TelefonoOpcional] [varchar](20) NULL,
	[Genero] [varchar](10) NULL,
	[Email] [varchar](50) NOT NULL,
	[Contraseña] [varchar](20) NOT NULL,
	[Ocupacion] [varchar](20) NOT NULL,
	[Parentesco] [bit] NULL,
	[Direccion_trabajo] [varchar](20) NOT NULL,
	[regMat] [int] NULL,
PRIMARY KEY CLUSTERED 
(
	[Id_Acudiente] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO
